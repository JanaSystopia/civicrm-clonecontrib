<?xml version="1.0"?>
<extension key="clonecontrib" type="module">
  <file>clonecontrib</file>
  <name>Clone Contributions</name>
  <description>Provides an API for cloning an existing contribution to a new one.</description>
  <license>GPL-3.0</license>
  <maintainer>
    <author>Allen Shaw</author>
    <email>allen@JoineryHQ.com</email>
  </maintainer>
  <urls>
    <url desc="Main Extension Page">https://github.com/twomice/civicrm-clonecontrib</url>
    <url desc="Documentation">https://twomice.github.io/civicrm-clonecontrib/</url>
    <url desc="Support">https://github.com/twomice/civicrm-clonecontrib/issues</url>
    <url desc="Licensing">http://www.gnu.org/licenses/gpl-3.0.html</url>
  </urls>
  <releaseDate>2020-05-14</releaseDate>
  <version>1.4.0</version>
  <develStage>stable</develStage>
  <compatibility>
    <ver>5.38</ver>
  </compatibility>
  <comments>Developed by Joinery for Korlon and TriMet.</comments>
  <civix>
    <namespace>CRM/Clonecontrib</namespace>
    <format>23.02.1</format>
  </civix>
  <mixins>
    <mixin>menu-xml@1.0.0</mixin>
    <mixin>setting-php@1.0.0</mixin>
    <mixin>smarty-v2@1.0.1</mixin>
  </mixins>
  <classloader>
    <psr0 prefix="CRM_" path="."/>
    <psr4 prefix="Civi\" path="Civi"/>
  </classloader>
  <upgrader>CRM_Clonecontrib_Upgrader</upgrader>
</extension>
